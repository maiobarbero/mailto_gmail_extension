{
  "manifest_version": 3,
  "name": "Mailto to Gmail in Browser",
  "version": "1.1",
  "description": "Open mailto links in Gmail instead of the default email client.",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "run_at": "document_start"
    }
  ],
  "action": {
    "default_title": "Mailto to Gmail in Browser"
  },
    "icons": {
        "48": "icon48.png",
        "128": "icon128.png"
    }
}
